{
  "name": "vite-rslib-ui",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "import": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "import": "./dist/cjs/index.cjs"
      }
    },
    "./*.css": {
      "import": "./dist/esm/static/css/*.css",
      "require": "./dist/cjs/static/css/*.css"
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib mf dev"
  },
  "dependencies": {
    "http-server": "14.1.1"
  },
  "devDependencies": {
    "@rsbuild/plugin-react": "^1.0.6",
    "@rslib/core": "^0.0.18",
    "@module-federation/rsbuild-plugin": "0.7.7",
    "@types/react": "^18.3.12",
    "react": "^18.3.1",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  }
}
